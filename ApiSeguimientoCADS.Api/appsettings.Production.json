{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "ApiSeguimientoCADS": "Information",
      "ApiSeguimientoCADS.Api.Controllers": "Information",
      "ApiSeguimientoCADS.Api.Handlers": "Information",
      "ApiSeguimientoCADS.Api.Infrastructure": "Warning"
    }
  },
  "NLogConfig": {
    "LogBasePath": "logs\\Production",
    "InternalLogLevel": "Error"
  },
  "Frontend": {
    "BaseUrl": "https://seguimientocads.bciseguros.cl/redirect/"
  },
  "Security": {
    "TokenSecretKey": "${TOKEN_SECRET_KEY}", // Variable de entorno
    "TokenExpirationMinutes": 30
  }
}